{
  "printWidth": 80,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "none",
  "semi": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "dotenv/config",
    "^react",
    "^vite",
    "<THIRD_PARTY_MODULES>",
    "^@maiar-ai/core",
    "^@maiar-ai/model(.*)$",
    "^@maiar-ai/memory(.*)$",
    "^@maiar-ai/plugin(.*)$",
    "^[./]",
    "^[../]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
